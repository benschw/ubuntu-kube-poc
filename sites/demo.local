server {
	listen   80; 
	listen   8000; 
	server_name demo.local;

	location / {
		# proxy_pass       http://172.20.20.10:8000;
		proxy_pass       http://11.1.1.81:80;

	    proxy_set_header Host            $host;
	    proxy_set_header X-Forwarded-For $remote_addr;

		index index.php index.html index.htm;
    }
}
